      // Open the selector of thickness
      // Entry: gnThickness=initial thickness
      PROCEDURE WD_PopupSelThickness(gnThickness is int=10)
      
      CONSTANT
      	nLOOPMINTHICKNESS = 2		// min thickness in the looper
      	nLOOPMAXTHICKNESS = 24	// max thickness in the looper
      	nLOOPSTEP = 2				// step of thickness in the looper
      END
   -
     code : |1+
      ModifThickness(gnThickness)
     type : 34
