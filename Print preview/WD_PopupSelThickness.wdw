  procedures :
   -
     name : ModifThickness
     internal_properties : BwAAAAcAAAC6S3R1VXnwufHFM7iF5fMiZuuFqyLOU+Fj2vX7kEC0VWXbD04ZvO0hD3TlwSppZRoYu4eMe1UKcS2Y5oZGvTImeDARJ4l9dauPZSOCCZwKg0dXvIgteotRdfbk2diTcRTxeV8hqDSEdECGgHCpDg==
     procedure_id : 1513987040002372436
     type_code : 14
     code : |1+
      // Summary: Modify the thickness in all the controls
      // Syntax:
      //ModifThickness (<nThickness> is int)
      //
      // Parameters:
      //	nThickness (integer): Thickness of the line
      // Return value:
      // 	None
      
      PROCEDURE ModifThickness(nThickness is int)
      
      // synchronizes the edit control
      COMBO_Thickness=nThickness
      
      // if the thickness is one of the preset values of the looper
      IF nThickness>nLOOPMINTHICKNESS AND nThickness<nLOOPMAXTHICKNESS AND (nThickness modulo nLOOPSTEP)=0 THEN
      	// subscript of the corresponding cell
      	nSubscript is int=(nThickness/nLOOPSTEP)
      	
      	// if the thickness selected in the looper does not correspond to the thickness to display
      	IF LOOP_Thickness<>nSubscript
      		// selects the proper cell in the looper
      		LOOP_Thickness=nSubscript
      		//		// refresh the looper
      		//		LooperDisplay(LOOP_Thickness,taCurrentFirst)
      	END
      ELSE
      	// deselects all
      	LooperSelectMinus(LOOP_Thickness)
      END
      
      // draws the circle in the image control
      DrawCircle(nThickness)
      
      // modify the returned value of the window
      MyWindow..ReturnedValue=nThickness
     type : 458752
   -
     name : DrawCircle
     procedure_id : 1513987040002437972
     type_code : 14
     code : |1+
      // Summary: Draws a circle in the image control
      // Syntax:
      //DrawCircle (<nThickness>)
      //
      // Parameters:
      // 	nThickness: diameter of the circle
      // Return value:
      // 	None
      PROCEDURE DrawCircle(nThickness)
      
      dStartDrawing(IMG_CurrentThickness)
      dCircle( IMG_CurrentThickness..Width/2-nThickness/2,
      IMG_CurrentThickness..Height/2-nThickness/2,
      IMG_CurrentThickness..Width/2+nThickness/2,
      IMG_CurrentThickness..Height/2+nThickness/2,
      iBlack,
      iBlack)
      //dEndDrawing(IMG_CurrentThickness)
      
     type : 458752
