      PROCEDURE WIN_LOGIN(LOCAL EMPRESA_ID_SELECIONADA IS 8 BYTE INT,LOCAL EMPRESA_NOME_SELECIONADA is string)
      USUARIO_ID = 0
      USUARIO_CONECTADO = ""
      ADMINISTRADOR = False	
      PODE_INCLUIR = False
      PODE_ALTERAR = False
      PODE_EXCLUIR = False
      EMAIL_DE_MENSAGEM = ""
      SENHA_EMAIL  = ""
      SERVIDOR_SMTP = ""
      PORTA_SMTP = ""	
      
      STC_NOME_EMPRESA = "EMPRESA: " + EMPRESA_NOME_SELECIONADA
   -
     code : |1-
      MyWindow..ReturnedValue = FALSE
      EDT_USUARIO = "admin"
      EDT_SENHA = "admin"
      IF HNbRec(LOGIN) = 0 THEN
      	HReset(LOGIN)	
      	LOGIN.NOME_USUARIO = "ADMINISTRADOR"
      	LOGIN.USUARIO = "admin"
      	login.SENHA = "admin"
      	LOGIN.ATIVO = true
      	LOGIN.ADMINISTRADOR = TRUE
      	LOGIN.PODE_INCLUIR = TRUE
      	LOGIN.PODE_ALTERAR = TRUE
      	LOGIN.PODE_EXCLUIR = TRUE
      	LOGIN.EMPRESA_ID = EMPRESA_ID_SELECIONADA
      	LOGIN.NOME_EMPRESA_UNIDADE = EMPRESA_NOME_SELECIONADA
      	LOGIN.CNPJ = CNPJ_EMPRESA
      	LOGIN.TELEFONE = TELEFONE_EMPRESA
      	LOGIN.ENDERECO = ENDERECO_EMPRESA + " " + BAIRRO_EMPRESA + " " + CIDADE_EMPRESA
      	LOGIN.LOGOMARCA = LOGOMARCA_EMPRESA
      	IF NOT HADD(LOGIN) THEN
      		NextTitle("ATENÇÃO!")
      	Warning("ERRO AO CRIAR O USUÁRIO PADRÃO!")	
      		RETURN
      	END
      END
      
