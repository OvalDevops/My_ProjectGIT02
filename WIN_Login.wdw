         				WIN_MENU.STC_USUARIO_CONECTADO = USUARIO_CONECTADO
         				WIN_MENU.STC_NOME_EMPRESA = NOME_EMPRESA
         				CLOSE(MyWindow)
         												
         			ELSE
         				USUARIO_ID = 0
         				USUARIO_CONECTADO = ""	
